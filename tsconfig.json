{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@cv": [
        "./cv.json"
      ],
      "@section/*": [
        "src/components/sections/*"
      ],
      "@react/*": [
        "src/components/react/*"
      ],
      "@*": [
        "src/*"
      ]
    },

  }
}